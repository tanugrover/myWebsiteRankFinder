
@model IEnumerable<InfoTrackSEOApp.Models.ResultsModel>


<h1>Search results are here:</h1>
<h3>Links found: @Model.Count()</h3>
@if (Model.Count() >0)
{

    <table border="1" cellpadding="1">
        <tr><td>Rank</td><td>Reference Found</td></tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Index)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.link)
                </td>
            </tr>

        }
    </table>
}
    <button type="button" onclick="location.href='@Url.Action("Index", "Home")'" name="Back to Search" value="Back to Search Page"> Back to Search</button>

